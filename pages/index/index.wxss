.channel-list {
  display: flex;
  height: 80rpx;
  background: #6399FF;
}

.channel-item {
  display: flex;
  width: 150rpx;
  flex-shrink: 0;
  flex-direction: column;
  align-items: center;
}

.channel-title {
  font-size: 40rpx;
  line-height: 56rpx;
}

.channel-underline {
  height: 10rpx;
  width: 50rpx;
  margin-top: 5rpx;
  background-color: #D8D8D8;
}

.news-wrapper {
  display: flex;
  flex-direction: column;
}

.news-panel {
  height: 280rpx;
}